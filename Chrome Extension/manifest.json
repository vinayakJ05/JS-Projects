{
    "name": "Color Picker",
    "description": "A color picker extension",
    "version": "1.0",
    "manifest_version": 3,
    "background":{
        "service_worker": "background.js"
    },
   
    "permissions":[
        "activeTab",
        "scripting"
    ],

    "action":{
        "default_popup":"popup.html"
    },
    
    "icons":{
        "16":"/images/favicon-16.png",
        "32":"images/favicon-32.png",
        "48":"images/favicon-48.png"
    }

    
}